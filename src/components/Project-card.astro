---
interface Props {
  title: string;
  link: string;
  description: string;
}


const { title, link, description } = Astro.props;
---

<div>
    <h3 class="text-blue-100">
        <a href={link}>
            {title}
        </a>
    </h3>
    <div>
        <p class="text-base text-blue-600">
            {description}
        </p>
    </div>
</div>

